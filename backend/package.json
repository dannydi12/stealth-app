{
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc",
    "build:w": "tsc --watch",
    "start": "node ./build/src/index.js",
    "watch": "concurrently --handle-input \"yarn build:w\" \"nodemon --delay 500ms --watch ./build/ --exec yarn start \""
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23"
  }
}
